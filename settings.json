{
  "service":{
      "name":"nuwa",
      "port":8080
  },
  "machine":{
    "name":"voldenuit",
    "extension":".local" 
  },
  "destinationPath":"nuwavideos",
  "output":{
    "bitrate": 3000
  },
  "simulateConnection":false,
  "maxImageNumber": 56,
  "composition":{
    "default":"assembly",
    "audio":false,
    "effect":[
        {
          "name":"video-layer",
          "source":"sunglasshut",
          "type": "clients",
          "basename":"%04d.png"
        },
        {
          "name":"assembly",
          "source":"auto",
          "type": "clients",
          "basename":"%*.jpg"
        }
    ]
  }
}
